      <event name="item:saved">
        <handler type="Sitecore.Marketing.xMgmt.Definitions.ItemEventHandler, Sitecore.Marketing.xMgmt" method="OnItemSaved"/>
      </event>
      <event name="item:saving">
        <handler type="Sitecore.Marketing.xMgmt.Definitions.ItemEventHandler, Sitecore.Marketing.xMgmt" method="OnItemSaving"/>
      </event>
      <event name="item:creating">
        <handler type="Sitecore.Marketing.xMgmt.Definitions.ItemEventHandler, Sitecore.Marketing.xMgmt" method="OnItemCreating"/>
      </event>
    </events>
    <fieldTypes>
      <fieldType name="JSON" type="Sitecore.Marketing.Data.Fields.JsonField, Sitecore.Marketing.xMgmt" />
    </fieldTypes>
    <pipelines>
      <deployDefinition>
        <processor type="Sitecore.Marketing.xMgmt.Pipelines.DeployDefinition.Deploy, Sitecore.Marketing.xMgmt" resolve="true"/>
      </deployDefinition>
      <validateJsonFieldLinks>
        <processor type="Sitecore.Marketing.Pipelines.ValidateJsonFieldLinks.ValidateProperties, Sitecore.Marketing.xMgmt"
